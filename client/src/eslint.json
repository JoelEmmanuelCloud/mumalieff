{
  "extends": [
    "react-app",
    "react-app/jest"
  ],
  "globals": {
    "gtag": "readonly"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off"
  },
  "overrides": [
    {
      "files": ["**/*.js", "**/*.jsx"],
      "globals": {
        "gtag": "readonly",
        "window": "readonly",
        "document": "readonly",
        "navigator": "readonly",
        "process": "readonly"
      }
    }
  ]
}